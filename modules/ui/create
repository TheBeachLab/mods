// closure
//
(function(){
//
// module globals
//
var mod = {}
//
// name
//
var name = 'create'
//
// initialization
//
var init = function() {
   }
//
// inputs
//
var inputs = {
   }
//
// outputs
//
var outputs = {
   }
//
// interface
//
var interface = function(div){
   mod.div = div
   //
   // create
   //
   var btn = document.createElement('button')
      btn.style.padding = mods.ui.padding
      btn.style.margin = 1
      btn.appendChild(document.createTextNode('create module'))
      btn.addEventListener('click',function(){
         mods.create("{\
            name:'asd',\
            init:function(){},\
            inputs:{},\
            outputs:{},\
            interface:function(){}\
            }")
            //interface:function(div){\
            //  mod.div = div\
            //   div.appendChild(document.createTextNode(' image '))\
            //   }\
         })
      div.appendChild(btn)
   }
//
// local functions
//

//
// return values
//
return ({
   name:name,
   init:init,
   inputs:inputs,
   outputs:outputs,
   interface:interface
   })
}())
